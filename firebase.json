{
  "firebase": "freya-planning-poker",
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist/freya-planning-poker",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }]
  }
}
